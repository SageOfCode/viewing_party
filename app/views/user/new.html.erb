<h1>Register</h1>

<%= form_with url: '/registration/new', local: true do |form| %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.label :confirm_password %>
  <%= form.password_field :confirm_password %>
  <%= form.submit 'Register' %>
<% end %>